<div id="body">
    <header>
        <div>
            <h5>Start</h5>
            <p>@resultDisplay.StartingPrice</p>
        </div>
        <div>
            <h5>Kr./km</h5>
            <p>@resultDisplay.PerKm</p>
        </div>
        <div>
            <h5>Kr./min</h5>
            <p>@resultDisplay.PerMin</p>
        </div>
        <div>
            <h5>Rejsetid</h5>
            <p>@resultDisplay.TravelTime</p>
        </div>
        <div>
            <h5>Længde</h5>
            <p>@resultDisplay.Distance</p>
        </div>
    </header>
    <button type="submit" form="Submitform">Beregn</button>
    <h5 id="ResultText">Rejsen koster i alt:</h5>
    <h3>@taxiCost kr.</h3>
</div>

